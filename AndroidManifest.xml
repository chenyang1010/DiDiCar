<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tt.servic"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tt.servic.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="com.tt.servic.MyService">         
            <intent-filter>
                <action android:name="com.tt.servic.MyService"></action>
            </intent-filter>
        </service>
        
         <service android:name="com.tt.servic.ActionService">         
            <intent-filter>
                <action android:name="com.tt.servic.ActionService"></action>
            </intent-filter>
        </service>
        
    </application>

</manifest>
